{
  "private": true,
  "name": "@munchkin/cordova",
  "type": "module",
  "targets": {
    "android": {
      "distDir": "www",
      "engines": {
        "browsers": "chrome >= 69"
      },
      "publicUrl": ".",
      "source": "src/index.html",
      "sourceMap": {
        "inline": true
      }
    },
    "ios": {
      "distDir": "www",
      "engines": {
        "browsers": "ios >= 13"
      },
      "publicUrl": ".",
      "source": "src/index.html",
      "sourceMap": {
        "inline": true
      }
    },
    "windows": {
      "distDir": "www",
      "engines": {
        "browsers": "edge >= 18"
      },
      "publicUrl": ".",
      "source": "src/index.html",
      "sourceMap": {
        "inline": true
      }
    }
  },
  "alias": {
    "@formatjs/icu-messageformat-parser": "@formatjs/icu-messageformat-parser/no-parser"
  },
  "scripts": {
    "build:android": "cordova build android --release",
    "build:ios": "cordova build ios",
    "build:www:android": "yarn run clean && yarn g:parcel build --target=android",
    "build:www:ios": "yarn run clean && yarn g:parcel build --target=ios",
    "build:www:windows": "yarn run clean && yarn g:parcel build --target=windows",
    "clean": "yarn g:rimraf www",
    "cordova:prepare:android": "cordova prepare android",
    "cordova:prepare:ios": "cordova prepare ios",
    "cordova:prepare:windows": "cordova prepare windows",
    "run:android": "cordova run android",
    "run:ios": "cordova run ios"
  },
  "cordova": {
    "platforms": [
      "android",
      "ios",
      "windows"
    ],
    "plugins": {
      "cordova-plugin-android-dark-mode-support": {},
      "cordova-plugin-buildinfo": {},
      "cordova-plugin-firebase-analytics": {
        "ANALYTICS_COLLECTION_ENABLED": "true",
        "AUTOMATIC_SCREEN_REPORTING_ENABLED": "true",
        "ANDROID_FIREBASE_BOM_VERSION": "30.3.1"
      },
      "cordova-plugin-firebase-crash": {
        "CRASHLYTICS_COLLECTION_ENABLED": "true",
        "ANDROID_FIREBASE_BOM_VERSION": "30.3.1"
      },
      "cordova-plugin-headercolor": {},
      "cordova-plugin-insomnia": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-localization-strings": {
        "TRANSLATION_PATH": "translations/app/"
      },
      "cordova-plugin-purchase": {
        "BILLING_KEY": ""
      },
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-vibration": {},
      "cordova-plugin-web-share": {}
    }
  },
  "dependencies": {
    "smoothscroll-polyfill": "0.4.4",
    "web-animations-js": "2.3.2"
  },
  "devDependencies": {
    "@types/smoothscroll-polyfill": "0.3.1",
    "@types/web-animations-js": "2.2.12",
    "cordova": "11.1.0",
    "cordova-android": "11.0.0",
    "cordova-ios": "6.2.0",
    "cordova-plugin-add-swift-support": "2.0.2",
    "cordova-plugin-android-dark-mode-support": "1.0.0",
    "cordova-plugin-buildinfo": "4.0.0",
    "cordova-plugin-firebase-analytics": "7.0.5",
    "cordova-plugin-firebase-crash": "7.0.4",
    "cordova-plugin-headercolor": "1.0.0",
    "cordova-plugin-insomnia": "4.3.0",
    "cordova-plugin-ionic-keyboard": "2.2.0",
    "cordova-plugin-localization-strings": "4.2.0",
    "cordova-plugin-purchase": "11.0.0",
    "cordova-plugin-splashscreen": "6.0.2",
    "cordova-plugin-statusbar": "3.0.0",
    "cordova-plugin-vibration": "3.1.1",
    "cordova-plugin-web-share": "1.3.0",
    "cordova-windows": "7.0.1"
  }
}
