{
  "name": "munchkin-cordova",
  "version": "0.0.0",
  "scripts": {
    "build:android": "cordova build android --release",
    "build:ios": "cordova build ios",
    "reinstall": "rm -rf node_modules platforms plugins package-lock.json && npm install && cordova prepare android && cordova prepare ios",
    "run:android": "cordova run android",
    "run:ios": "cordova run ios"
  },
  "cordova": {
    "platforms": [
      "android",
      "ios",
      "windows"
    ],
    "plugins": {
      "cordova-plugin-buildinfo": {},
      "cordova-plugin-firebase-analytics": {
        "FIREBASE_ANALYTICS_COLLECTION_ENABLED": "true",
        "FIREBASE_ANALYTICS_VERSION": "17.4.+"
      },
      "cordova-plugin-firebase-crash": {
        "FIREBASE_CRASHLYTICS_COLLECTION_ENABLED": "true",
        "FIREBASE_CRASHLYTICS_VERSION": "17.0.0"
      },
      "cordova-plugin-headercolor": {},
      "cordova-plugin-insomnia": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-localization-strings": {
        "TRANSLATION_PATH": "translations/app/"
      },
      "cordova-plugin-purchase": {
        "BILLING_KEY": ""
      },
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-theme-detection": {},
      "cordova-plugin-vibration": {},
      "cordova-plugin-web-share": {},
      "cordova-plugin-whitelist": {},
      "cordova-support-google-services": {}
    }
  },
  "devDependencies": {
    "cordova-android": "9.0.0",
    "cordova-ios": "6.1.1",
    "cordova-plugin-add-swift-support": "2.0.2",
    "cordova-plugin-buildinfo": "4.0.0",
    "cordova-plugin-firebase-analytics": "5.0.0",
    "cordova-plugin-firebase-crash": "3.0.0",
    "cordova-plugin-headercolor": "1.0.0",
    "cordova-plugin-insomnia": "4.3.0",
    "cordova-plugin-ionic-keyboard": "2.2.0",
    "cordova-plugin-localization-strings": "3.2.1",
    "cordova-plugin-purchase": "10.5.3",
    "cordova-plugin-splashscreen": "6.0.0",
    "cordova-plugin-statusbar": "2.4.3",
    "cordova-plugin-theme-detection": "1.3.0",
    "cordova-plugin-vibration": "3.1.1",
    "cordova-plugin-web-share": "1.2.0",
    "cordova-plugin-whitelist": "1.3.4",
    "cordova-support-google-services": "1.4.1",
    "cordova-windows": "7.0.1"
  }
}
