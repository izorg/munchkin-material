{
  "private": true,
  "name": "@munchkin/cordova",
  "type": "commonjs",
  "sideEffects": [
    "src/cordova.tsx",
    "src/index.ts",
    "src/polyfills.ts",
    "src/sentry.ts"
  ],
  "scripts": {
    "build:android": "cordova build android --release",
    "build:ios": "cordova build ios",
    "build": "yarn parcel build --dist-dir www --no-cache src/index.html",
    "build:www": "rm -rf www && yarn run build && rm -r www/*.map",
    "cordova:prepare": "cordova prepare",
    "run:android": "cordova run android",
    "run:ios": "cordova run ios"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@mui/material": "6.1.8",
    "@sentry/babel-plugin-component-annotate": "2.22.6",
    "@sentry/react": "8.40.0",
    "@types/cordova": "11.0.3",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/smoothscroll-polyfill": "0.3.4",
    "abortcontroller-polyfill": "1.7.6",
    "cordova": "12.0.0",
    "cordova-android": "13.0.0",
    "cordova-ios": "7.1.1",
    "cordova-plugin-buildinfo": "4.0.0",
    "cordova-plugin-firebase-analytics": "8.0.0",
    "cordova-plugin-firebase-crash": "8.0.2",
    "cordova-plugin-headercolor": "1.0.0",
    "cordova-plugin-insomnia": "4.3.0",
    "cordova-plugin-ionic-keyboard": "2.2.0",
    "cordova-plugin-localization-strings": "5.0.6",
    "cordova-plugin-purchase": "13.11.1",
    "cordova-plugin-statusbar": "4.0.0",
    "cordova-plugin-vibration": "3.1.1",
    "core-js": "3.39.0",
    "parcel": "2.13.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router": "7.0.1",
    "smoothscroll-polyfill": "0.4.4",
    "swr": "2.2.5"
  },
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-buildinfo": {},
      "cordova-plugin-firebase-analytics": {
        "ANALYTICS_COLLECTION_ENABLED": "true",
        "ANDROID_FIREBASE_BOM_VERSION": "33.6.0",
        "AUTOMATIC_SCREEN_REPORTING_ENABLED": "false",
        "IOS_FIREBASE_POD_VERSION": "11.5.0"
      },
      "cordova-plugin-firebase-crash": {
        "ANDROID_FIREBASE_BOM_VERSION": "33.6.0",
        "CRASHLYTICS_COLLECTION_ENABLED": "true",
        "IOS_FIREBASE_POD_VERSION": "11.5.0"
      },
      "cordova-plugin-headercolor": {},
      "cordova-plugin-insomnia": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-localization-strings": {
        "TRANSLATION_PATH": "translations/app/"
      },
      "cordova-plugin-purchase": {
        "BILLING_KEY": ""
      },
      "cordova-plugin-statusbar": {},
      "cordova-plugin-vibration": {}
    }
  },
  "alias": {
    "@formatjs/icu-messageformat-parser": "@formatjs/icu-messageformat-parser/no-parser"
  }
}
