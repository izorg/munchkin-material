{
  "private": true,
  "name": "@munchkin/windows",
  "sideEffects": [
    "src/index.tsx"
  ],
  "targets": {
    "default": {
      "distDir": "Munchkin/Munchkin/Assets/www",
      "source": "src/index.html"
    }
  },
  "scripts": {
    "build": "parcel build",
    "build:www": "yarn g:rimraf Munchkin/Munchkin/Assets/www && yarn build --no-cache && yarn g:rimraf Munchkin/Munchkin/Assets/www/*.map"
  },
  "devDependencies": {
    "babel-preset-web": "workspace:*",
    "parcel": "2.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router": "7.12.0"
  }
}
