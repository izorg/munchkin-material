{
  "private": true,
  "name": "@munchkin/windows",
  "sideEffects": [
    "src/index.tsx"
  ],
  "targets": {
    "default": {
      "distDir": "Munchkin/Assets/www",
      "source": "src/index.html"
    }
  },
  "scripts": {
    "build": "parcel build",
    "build:www": "yarn g:rimraf Munchkin/Assets/www && yarn build --no-cache && yarn g:rimraf Munchkin/Assets/www/*.map"
  },
  "devDependencies": {
    "parcel": "2.14.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router": "7.5.3"
  }
}
