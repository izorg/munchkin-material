{
  "name": "@munchkin/windows",
  "private": true,
  "sideEffects": [
    "src/index.ts",
    "src/polyfills.ts"
  ],
  "targets": {
    "default": {
      "distDir": "Munchkin/Munchkin/Assets/www",
      "source": "src/index.html"
    }
  },
  "scripts": {
    "build": "parcel build",
    "build:www": "yarn g:rimraf Munchkin/Munchkin/Assets/www && yarn build --no-cache && yarn g:rimraf Munchkin/Munchkin/Assets/www/*.map"
  },
  "devDependencies": {
    "@munchkin/babel-preset-web": "workspace:*",
    "core-js": "3.48.0",
    "parcel": "2.16.4",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-router": "7.13.0"
  }
}
