    "@dnd-kit/utilities": "3.2.2",
    "@emotion/cache": "11.13.5",
    "@emotion/react": "11.13.5",
    "@emotion/styled": "11.13.5",
    "@formatjs/cli": "6.3.11",
    "@formatjs/intl-getcanonicallocales": "2.5.3",
    "@formatjs/intl-locale": "4.2.5",
    "@formatjs/intl-numberformat": "8.14.5",
    "@formatjs/intl-pluralrules": "5.3.5",
    "@jest/globals": "29.7.0",
    "@mdi/js": "7.4.47",
    "@mui/material": "6.1.9",
    "@mui/types": "7.2.19",
    "@mui/utils": "6.1.9",
    "@parcel/config-default": "2.13.2",
    "@parcel/core": "2.13.2",
    "@parcel/packager-raw-url": "2.13.2",
    "@parcel/reporter-bundle-analyzer": "2.13.2",
    "@parcel/reporter-bundle-buddy": "2.13.2",
