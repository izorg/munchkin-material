{
  "private": true,
  "name": "@munchkin/web",
  "type": "module",
  "sideEffects": [
    "src/dev/index.ts",
    "src/firebase.ts",
    "src/index.tsx",
    "src/polyfills.ts",
    "src/sentry.ts"
  ],
  "scripts": {
    "analyze": "yarn run build:parcel --reporter @parcel/reporter-bundle-analyzer",
    "build": "yarn run generate-assets && yarn run build:parcel && yarn run build:workbox",
    "build:parcel": "yarn g:rimraf ../../.parcel-cache dist && yarn parcel build src/index.html src/localized-files/*/index.html",
    "build:workbox": "workbox generateSW workbox.config.json",
    "dev": "yarn parcel --port=3000 src/index.html",
    "generate-assets": "node scripts/generate-assets.js",
    "i18n:compile": "formatjs compile-folder --ast --format=simple l10n l10n/generated",
    "i18n:extract": "formatjs extract --format=simple --out-file=l10n/en.json \"src/**/*.{js,ts,tsx}\"",
    "screenshots": "node --loader ts-node/esm --no-warnings scripts/screenshots.ts",
    "start": "npx firebase-tools emulators:start"
  },
  "devDependencies": {
    "@dnd-kit/core": "6.1.0",
    "@dnd-kit/modifiers": "7.0.0",
    "@dnd-kit/sortable": "8.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@formatjs/cli": "6.2.12",
    "@formatjs/intl-datetimeformat": "6.12.5",
    "@formatjs/intl-getcanonicallocales": "2.3.0",
    "@formatjs/intl-locale": "4.0.0",
    "@formatjs/intl-numberformat": "8.10.3",
    "@formatjs/intl-pluralrules": "5.2.14",
    "@mdi/js": "7.4.47",
    "@mui/material": "5.16.4",
    "@mui/types": "7.2.15",
    "@mui/utils": "5.16.4",
    "@parcel/config-default": "2.12.0",
    "@parcel/core": "2.12.0",
    "@parcel/packager-raw-url": "2.12.0",
    "@parcel/reporter-bundle-analyzer": "2.12.0",
    "@parcel/transformer-webmanifest": "2.12.0",
    "@reduxjs/toolkit": "2.2.6",
    "@sentry/react": "8.18.0",
    "@types/dom-screen-wake-lock": "1.0.3",
    "@types/node": "20.14.11",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/stylis": "4.2.6",
    "@types/uuid": "10.0.0",
    "core-js": "3.37.1",
    "firebase": "10.12.3",
    "framer-motion": "11.3.6",
    "handlebars": "4.7.8",
    "parcel": "2.12.0",
    "playwright": "1.45.2",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-intl": "6.6.8",
    "react-redux": "9.1.2",
    "react-router-dom": "6.25.1",
    "react-transition-group": "4.4.5",
    "redux": "5.0.1",
    "redux-undo": "1.1.0",
    "stylis": "4.3.2",
    "stylis-plugin-rtl": "2.1.1",
    "swr": "2.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.5.3",
    "uuid": "10.0.0",
    "workbox-cli": "7.1.0",
    "workbox-window": "7.1.0"
  },
  "alias": {
    "@formatjs/icu-messageformat-parser": "@formatjs/icu-messageformat-parser/no-parser"
  }
}
