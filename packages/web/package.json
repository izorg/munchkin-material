{
  "private": true,
  "name": "@munchkin/web",
  "type": "module",
  "scripts": {
    "analyze": "yarn run build:parcel --reporter @parcel/reporter-bundle-analyzer",
    "build": "yarn run build:parcel && yarn run build:workbox",
    "build:parcel": "yarn g:rimraf dist && yarn g:parcel build",
    "build:workbox": "workbox generateSW workbox.config.json",
    "dev": "yarn g:parcel --port=3000",
    "start": "npx firebase-tools emulators:start"
  },
  "dependencies": {
    "@dnd-kit/core": "6.1.0",
    "@dnd-kit/modifiers": "7.0.0",
    "@dnd-kit/sortable": "8.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@formatjs/intl-datetimeformat": "6.11.2",
    "@formatjs/intl-getcanonicallocales": "2.3.0",
    "@formatjs/intl-locale": "3.4.1",
    "@formatjs/intl-numberformat": "8.8.1",
    "@formatjs/intl-pluralrules": "5.2.8",
    "@mdi/js": "7.3.67",
    "@mui/material": "5.14.17",
    "@mui/utils": "5.14.17",
    "@reduxjs/toolkit": "1.9.7",
    "@sentry/react": "7.77.0",
    "core-js": "3.33.2",
    "firebase": "10.5.2",
    "framer-motion": "10.16.4",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.5.2",
    "react-redux": "8.1.3",
    "react-router-dom": "6.18.0",
    "react-transition-group": "4.4.5",
    "redux": "4.2.1",
    "redux-undo": "1.1.0",
    "scrollyfills": "1.0.0",
    "stylis": "4.3.0",
    "stylis-plugin-rtl": "2.1.1",
    "swr": "2.2.4",
    "uuid": "9.0.1",
    "workbox-window": "7.0.0"
  },
  "devDependencies": {
    "@mui/types": "7.2.8",
    "@types/dom-screen-wake-lock": "1.0.3",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@types/stylis": "4.2.3",
    "@types/uuid": "9.0.7",
    "process": "0.11.10",
    "typescript": "5.2.2",
    "workbox-cli": "7.0.0"
  },
  "alias": {
    "@formatjs/icu-messageformat-parser": "@formatjs/icu-messageformat-parser/no-parser"
  },
  "targets": {
    "default": {
      "source": "src/index.html"
    }
  }
}
