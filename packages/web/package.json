{
  "private": true,
  "name": "@munchkin/web",
  "type": "module",
  "sideEffects": [
    "src/dev/index.ts",
    "src/firebase.ts",
    "src/index.tsx",
    "src/polyfills.ts",
    "src/sentry.ts"
  ],
  "targets": {
    "default": {
      "distDir": "dist",
      "source": [
        "src/index.html",
        "src/localized-files/*/index.html"
      ]
    }
  },
  "scripts": {
    "analyze": "yarn build:parcel --no-cache --reporter=@parcel/reporter-bundle-analyzer --reporter=@parcel/reporter-bundle-buddy",
    "build": "yarn generate-assets && rm -rf dist && yarn build:parcel --no-cache && yarn build:workbox",
    "build:parcel": "parcel build",
    "build:workbox": "workbox generateSW workbox.config.json",
    "dev": "parcel --port=3000",
    "generate-assets": "node scripts/generate-assets.js",
    "i18n:compile": "formatjs compile-folder --ast --format=simple l10n l10n/generated",
    "i18n:extract": "formatjs extract --format=simple --out-file=l10n/en.json \"src/**/*.{js,ts,tsx}\"",
    "screenshots": "playwright test",
    "start": "firebase-tools emulators:start",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "7.27.4",
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/modifiers": "9.0.0",
    "@dnd-kit/sortable": "10.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@formatjs/cli": "6.7.1",
    "@formatjs/intl-datetimeformat": "6.18.0",
    "@formatjs/intl-getcanonicallocales": "2.5.5",
    "@formatjs/intl-locale": "4.2.11",
    "@formatjs/intl-localematcher": "0.6.1",
    "@formatjs/intl-numberformat": "8.15.4",
    "@formatjs/intl-pluralrules": "5.4.4",
    "@jest/globals": "30.0.0",
    "@mdi/js": "7.4.47",
    "@mui/material": "7.1.1",
    "@mui/utils": "7.1.1",
    "@parcel/config-default": "2.15.2",
    "@parcel/core": "2.15.2",
    "@parcel/packager-raw-url": "2.15.2",
    "@parcel/reporter-bundle-analyzer": "2.15.2",
    "@parcel/reporter-bundle-buddy": "2.15.2",
    "@parcel/transformer-webmanifest": "2.15.2",
    "@playwright/test": "1.53.0",
    "@react-aria/interactions": "3.25.3",
    "@react-aria/utils": "3.29.1",
    "@reduxjs/toolkit": "2.8.2",
    "@sentry/babel-plugin-component-annotate": "3.5.0",
    "@sentry/react": "9.28.0",
    "@swc/core": "1.12.0",
    "@swc/jest": "0.2.38",
    "@types/node": "22.15.31",
    "@types/parcel-env": "0.0.9",
    "@types/react": "19.1.7",
    "@types/react-dom": "19.1.6",
    "@types/stylis": "4.2.7",
    "core-js": "3.43.0",
    "firebase": "11.9.1",
    "firebase-tools": "14.6.0",
    "handlebars": "4.7.8",
    "jest": "30.0.0",
    "parcel": "2.15.2",
    "playwright": "1.53.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-intl": "7.1.11",
    "react-redux": "9.2.0",
    "react-router": "7.6.2",
    "react-transition-group": "4.4.5",
    "redux": "5.0.1",
    "redux-undo": "1.1.0",
    "stylis": "4.3.6",
    "stylis-plugin-rtl": "2.1.1",
    "typescript": "5.8.3",
    "uuid": "11.1.0",
    "workbox-cli": "7.3.0",
    "workbox-window": "7.3.0"
  }
}
