{
  "private": true,
  "name": "@munchkin/web",
  "type": "module",
  "sideEffects": [
    "src/dev/index.ts",
    "src/firebase.ts",
    "src/index.tsx",
    "src/polyfills.ts",
    "src/sentry.ts"
  ],
  "scripts": {
    "analyze": "yarn run build:parcel --reporter @parcel/reporter-bundle-analyzer",
    "build": "yarn run generate-assets && yarn run build:parcel && yarn run build:workbox",
    "build:parcel": "yarn parcel build --no-cache src/index.html src/localized-files/*/index.html",
    "build:workbox": "workbox generateSW workbox.config.json",
    "dev": "yarn parcel --no-cache --port=3000 --watch-dir ../ src/index.html",
    "generate-assets": "node scripts/generate-assets.js",
    "i18n:compile": "formatjs compile-folder --ast --format=simple l10n l10n/generated",
    "i18n:extract": "formatjs extract --format=simple --out-file=l10n/en.json \"src/**/*.{js,ts,tsx}\"",
    "screenshots": "playwright test",
    "start": "npx firebase-tools emulators:start",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@dnd-kit/core": "6.1.0",
    "@dnd-kit/modifiers": "7.0.0",
    "@dnd-kit/sortable": "8.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@emotion/cache": "11.13.1",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@formatjs/cli": "6.3.9",
    "@formatjs/intl-datetimeformat": "6.16.4",
    "@formatjs/intl-getcanonicallocales": "2.5.2",
    "@formatjs/intl-locale": "4.2.4",
    "@formatjs/intl-numberformat": "8.14.4",
    "@formatjs/intl-pluralrules": "5.3.4",
    "@jest/globals": "29.7.0",
    "@mdi/js": "7.4.47",
    "@mui/material": "6.1.7",
    "@mui/types": "7.2.19",
    "@mui/utils": "6.1.7",
    "@parcel/config-default": "2.13.0",
    "@parcel/core": "2.13.0",
    "@parcel/packager-raw-url": "2.13.0",
    "@parcel/reporter-bundle-analyzer": "2.13.0",
    "@parcel/transformer-webmanifest": "2.13.0",
    "@playwright/test": "1.48.2",
    "@reduxjs/toolkit": "2.3.0",
    "@sentry/babel-plugin-component-annotate": "2.22.6",
    "@sentry/react": "8.38.0",
    "@swc/core": "1.9.2",
    "@swc/jest": "0.2.37",
    "@types/node": "22.9.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/stylis": "4.2.6",
    "core-js": "3.39.0",
    "firebase": "11.0.2",
    "handlebars": "4.7.8",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "motion": "11.11.17",
