{
  "private": true,
  "name": "@munchkin/web",
  "type": "module",
  "sideEffects": [
    "src/dev/index.ts",
    "src/firebase.ts",
    "src/index.tsx",
    "src/polyfills.ts",
    "src/sentry.ts"
  ],
  "scripts": {
    "analyze": "yarn run build:parcel --reporter @parcel/reporter-bundle-analyzer",
    "build": "yarn run generate-assets && yarn run build:parcel && yarn run build:workbox",
    "build:parcel": "yarn g:rimraf ../../.parcel-cache dist && yarn g:parcel build src/index.html src/localized-files/*/index.html",
    "build:workbox": "workbox generateSW workbox.config.json",
    "dev": "yarn g:parcel --port=3000 src/index.html",
    "generate-assets": "node scripts/generate-assets.js",
    "i18n:compile": "formatjs compile-folder --ast --format=simple l10n l10n/generated",
    "i18n:extract": "formatjs extract --format=simple --out-file=l10n/en.json \"src/**/*.{js,ts,tsx}\"",
    "start": "npx firebase-tools emulators:start"
  },
  "dependencies": {
    "@dnd-kit/core": "6.1.0",
    "@dnd-kit/modifiers": "7.0.0",
    "@dnd-kit/sortable": "8.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@formatjs/intl-datetimeformat": "6.12.2",
    "@formatjs/intl-getcanonicallocales": "2.3.0",
    "@formatjs/intl-locale": "3.4.5",
    "@formatjs/intl-numberformat": "8.10.0",
    "@formatjs/intl-pluralrules": "5.2.12",
    "@mdi/js": "7.4.47",
    "@mui/material": "5.15.10",
    "@mui/utils": "5.15.9",
    "@reduxjs/toolkit": "2.2.1",
    "@sentry/react": "7.102.0",
    "core-js": "3.36.0",
    "firebase": "10.8.0",
    "framer-motion": "11.0.5",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.6.2",
    "react-redux": "9.1.0",
    "react-router-dom": "6.22.1",
    "react-transition-group": "4.4.5",
    "redux": "5.0.1",
    "redux-undo": "1.1.0",
    "scrollyfills": "1.0.0",
    "stylis": "4.3.1",
    "stylis-plugin-rtl": "2.1.1",
    "swr": "2.2.5",
    "uuid": "9.0.1",
    "workbox-window": "7.0.0"
  },
  "devDependencies": {
    "@formatjs/cli": "6.2.7",
    "@mui/types": "7.2.13",
    "@types/dom-screen-wake-lock": "1.0.3",
    "@types/react": "18.2.57",
    "@types/react-dom": "18.2.19",
    "@types/stylis": "4.2.5",
    "@types/uuid": "9.0.8",
    "handlebars": "4.7.8",
    "process": "0.11.10",
    "typescript": "5.3.3",
    "workbox-cli": "7.0.0"
  },
  "alias": {
    "@formatjs/icu-messageformat-parser": "@formatjs/icu-messageformat-parser/no-parser"
  }
}
