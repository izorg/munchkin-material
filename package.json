{
  "name": "munchkin-web",
  "version": "12.1.3",
  "scripts": {
    "build:cordova": "cross-env NODE_ENV=production BUILD=cordova webpack",
    "build:web": "cross-env NODE_ENV=production BUILD=web webpack",
    "clean": "rm -rf coverage dist messages node_modules/.cache screenshots web",
    "dev": "cross-env NODE_ENV=development BUILD=web webpack-dev-server",
    "i18n:compile": "node -r dotenv/config scripts/i18n/download.mjs && formatjs compile-folder --ast --format simple languages languages/compiled",
    "i18n:extract": "formatjs extract --out-file messages.json \"src/**/*.js\" \"src/**/*.jsx\" && formatjs compile --out-file languages/en.json messages.json && rm messages.json",
    "lint": "eslint . --ext .js,.json,.jsx,.mjs",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "screenshots": "cross-env NODE_ENV=test node scripts/screenshots.js",
    "start": "cross-env NODE_ENV=production BUILD=web webpack-dev-server",
    "stats": "cross-env STATS=true npm run build:web",
    "test": "jest"
  },
  "dependencies": {
