{
  "private": true,
  "version": "14.7.3",
  "type": "module",
  "packageManager": "yarn@4.0.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rimraf .eslintcache .parcel-cache .stylelintcache coverage messages node_modules/.cache parcel-bundle-reports screenshots tsconfig.tsbuildinfo",
    "eslint": "NODE_OPTIONS=\"--max-old-space-size=4096\" eslint --cache \"**/*.{js,json,ts,tsx}\"",
    "i18n:compile": "formatjs compile-folder --ast --format=simple languages languages/generated",
    "i18n:extract": "formatjs extract --out-file=messages.json \"packages/web/src/**/*.{js,ts,tsx}\" && formatjs compile --out-file=languages/en.json messages.json && rm messages.json && prettier --write languages/en.json",
    "lint": "yarn run tsc && yarn run eslint && yarn run stylelint",
    "postinstall": "husky install",
    "prettier": "prettier --cache --ignore-unknown --write .",
    "reinstall": "rimraf node_modules yarn.lock && yarn install",
    "screenshots": "node scripts/screenshots.js",
    "stylelint": "stylelint --cache \"**/*.css\"",
    "test": "jest",
    "tsc": "tsc",
    "g:formatjs": "cd $INIT_CWD && formatjs",
    "g:parcel": "cd $INIT_CWD && parcel",
    "g:rimraf": "cd $INIT_CWD && rimraf"
  },
  "devDependencies": {
    "@formatjs/cli": "6.2.2",
    "@jest/globals": "29.7.0",
    "@parcel/packager-raw-url": "2.10.2",
    "@parcel/reporter-bundle-analyzer": "2.10.2",
    "@parcel/transformer-webmanifest": "2.10.2",
    "@swc/core": "1.3.96",
    "@swc/jest": "0.2.29",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "axios": "1.6.1",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-perfectionist": "2.2.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "5.0.0-canary-7118f5dd7-20230705",
    "eslint-plugin-unicorn": "49.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.0.2",
    "parcel": "2.10.2",
    "playwright": "1.39.0",
    "postcss": "8.4.31",
    "postcss-html": "1.5.0",
    "postcss-syntax": "0.36.2",
    "prettier": "3.0.3",
    "process": "0.11.10",
    "rimraf": "5.0.5",
    "stylelint": "15.11.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "6.0.3",
    "stylelint-prettier": "4.0.2",
    "typescript": "5.2.2"
  },
  "resolutions": {
    "@parcel/runtime-js@npm:2.10.1": "patch:@parcel/runtime-js@npm%3A2.10.1#~/.yarn/patches/@parcel-runtime-js-npm-2.10.1-3957858a1b.patch"
  }
}
