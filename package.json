{
  "private": true,
  "name": "munchkin",
  "version": "16.1.4",
  "type": "module",
  "packageManager": "yarn@4.12.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean-cache": "rm -rf .parcel-cache",
    "format": "prettier --cache --ignore-unknown --write .",
    "get-version": "node --no-warnings scripts/version.js",
    "lint": "yarn lint-unused && yarn lint-types && yarn lint-js && yarn lint-cycle && yarn lint-css",
    "lint-cycle": "oxlint --import-plugin -A all -D import/no-cycle",
    "lint-css": "stylelint --cache \"**/*.css\"",
    "lint-js": "eslint --cache \"**/*.{js,ts,tsx}\"",
    "lint-types": "tsc",
    "lint-unused": "knip --cache",
    "postinstall": "husky || true",
    "reinstall": "rm -r node_modules yarn.lock && yarn install",
    "g:rimraf": "cd $INIT_CWD && rimraf"
  },
  "devDependencies": {
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
    "@eslint/js": "9.39.2",
    "@eslint/json": "0.14.0",
    "@next/eslint-plugin-next": "16.1.1",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/github": "12.0.2",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@types/node": "24.10.4",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "eslint": "9.39.2",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-compat": "6.0.2",
    "eslint-plugin-formatjs": "6.0.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.10.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-perfectionist": "5.1.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-unicorn": "62.0.0",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "knip": "5.76.3",
    "lint-staged": "16.2.7",
    "oxlint": "1.35.0",
    "prettier": "3.7.4",
    "rimraf": "6.1.2",
    "semantic-release": "25.0.2",
    "stylelint": "16.26.1",
    "stylelint-config-standard": "39.0.1",
    "stylelint-order": "7.0.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.1"
  },
  "alias": {
    "@formatjs/icu-messageformat-parser": "@formatjs/icu-messageformat-parser/no-parser.js",
    "process": false
  },
  "@parcel/resolver-default": {
    "packageExports": true
  }
}
