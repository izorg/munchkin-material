    "react": "17.0.1",
    "react-beautiful-dnd": "13.0.0",
    "react-dom": "17.0.1",
    "react-intl": "5.13.2",
    "react-query": "3.12.0",
    "react-redux": "7.2.2",
    "react-router-dom": "6.0.0-beta.0",
    "react-sortablejs": "6.0.0",
