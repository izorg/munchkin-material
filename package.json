{
  "private": true,
  "name": "munchkin",
  "version": "16.0.1",
  "type": "module",
  "packageManager": "yarn@4.9.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean-cache": "rm -rf .parcel-cache",
    "format": "prettier --cache --ignore-unknown --write .",
    "get-version": "node --no-warnings scripts/version.js",
    "lint": "yarn lint-unused && yarn lint-types && yarn lint-js && yarn lint-circular && yarn lint-css",
    "lint-circular": "madge --circular packages/cordova/src/index.ts packages/site/app packages/web/src/index.tsx",
    "lint-css": "stylelint --cache \"**/*.css\"",
    "lint-js": "eslint --cache \"**/*.{js,ts,tsx}\"",
    "lint-types": "tsc",
    "lint-unused": "knip --cache",
    "postinstall": "husky || true",
    "reinstall": "rm -r node_modules yarn.lock && yarn install",
    "g:rimraf": "cd $INIT_CWD && rimraf"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@eslint-community/eslint-plugin-eslint-comments": "4.4.1",
    "@eslint/js": "9.24.0",
    "@eslint/json": "0.10.0",
    "@next/eslint-plugin-next": "15.3.0",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/github": "11.0.1",
    "@semantic-release/release-notes-generator": "14.0.3",
    "@types/node": "22.14.1",
    "conventional-changelog-conventionalcommits": "8.0.0",
    "eslint": "9.24.0",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-import-resolver-typescript": "4.3.2",
    "eslint-plugin-compat": "6.0.2",
    "eslint-plugin-formatjs": "5.3.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-perfectionist": "4.11.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-unicorn": "58.0.0",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "knip": "5.50.2",
    "lint-staged": "15.5.1",
    "madge": "8.0.0",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "semantic-release": "24.2.3",
    "stylelint": "16.18.0",
    "stylelint-config-standard": "38.0.0",
    "stylelint-order": "6.0.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.1"
  },
  "alias": {
    "@formatjs/icu-messageformat-parser": "@formatjs/icu-messageformat-parser/no-parser",
    "@mui/material": "@mui/material/esm",
    "@mui/material-nextjs": "@mui/material-nextjs/esm",
    "@mui/private-theming": "@mui/private-theming/esm",
    "@mui/styled-engine": "@mui/styled-engine/esm",
    "@mui/system": "@mui/system/esm",
    "@mui/types": "@mui/types/esm",
    "@mui/utils": "@mui/utils/esm",
    "process": false
  }
}
