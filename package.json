    "firebase": "9.1.3",
    "framer-motion": "4.1.17",
    "history": "5.0.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.20.12",
    "react-redux": "7.2.5",
    "react-router-dom": "6.0.0-beta.6",
    "react-transition-group": "4.4.2",
    "redux": "4.1.1",
