{
  "name": "munchkin-web",
  "version": "8.4.1",
  "main": "dist/js/app.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf coverage dist messages node_modules/.cache screenshots site",
    "dev": "cross-env NODE_ENV=development BUILD=site webpack-dev-server",
    "dist": "cross-env NODE_ENV=production BUILD=dist webpack",
    "i18n:download": "node scripts/i18n/download.js",
    "i18n:translate": "cross-env BABEL_ENV=i18n babel src --out-dir temp && node scripts/i18n/translate.js && rimraf temp messages",
    "i18n:upload": "node scripts/i18n/upload.js",
    "lint": "eslint . --ext .js,.json,.jsx",
    "precommit": "lint-staged && npm test",
    "postcommit": "git update-index --again",
    "prepush": "npm run lint && npm test",
    "screenshots": "babel-node scripts/screenshots.js",
    "site": "cross-env NODE_ENV=production BUILD=site webpack",
    "start": "cross-env NODE_ENV=production BUILD=site webpack-dev-server",
    "stats": "cross-env STATS=true npm run site",
    "test": "jest"
  },
  "lint-staged": {
    "**/*.{js,json,jsx}": [
      "eslint --ext .js,.json,.jsx --fix",
      "git add"
    ],
    "**/*.{md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
