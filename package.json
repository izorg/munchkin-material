{
  "private": true,
  "version": "14.5.0",
  "type": "module",
  "workspaces": [
    "cordova",
    "web"
  ],
  "scripts": {
    "clean": "rimraf .eslintcache .parcel-cache .stylelintcache coverage messages node_modules/.cache parcel-bundle-reports screenshots tsconfig.tsbuildinfo",
    "eslint": "eslint --cache \"**/*.{js,json,ts,tsx}\"",
    "i18n:compile": "formatjs compile-folder --ast --format=simple languages languages/generated",
    "i18n:extract": "formatjs extract --out-file=messages.json \"web/src/**/*.{js,ts,tsx}\" && formatjs compile --out-file=languages/en.json messages.json && rm messages.json && prettier --write languages/en.json",
    "lint": "yarn run tsc && yarn run eslint && yarn run stylelint",
    "postinstall": "husky install",
    "prettier": "prettier --cache --ignore-unknown --write .",
    "reinstall": "rimraf node_modules yarn.lock && yarn install",
    "screenshots": "node scripts/screenshots.js",
    "stylelint": "stylelint --cache \"**/*.css\"",
    "test": "jest",
    "tsc": "tsc",
    "g:rimraf": "cd $INIT_CWD && rimraf"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@emotion/babel-plugin": "11.10.8",
    "@formatjs/cli": "6.1.1",
    "@jest/globals": "29.5.0",
    "@parcel/transformer-webmanifest": "2.8.3",
    "@swc/core": "1.3.56",
    "@swc/jest": "0.2.26",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "axios": "1.4.0",
    "babel-plugin-formatjs": "10.5.1",
    "babel-plugin-polyfill-corejs3": "0.7.1",
    "babel-plugin-react-remove-properties": "0.3.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "13.2.2",
    "playwright": "1.33.0",
    "postcss-html": "1.5.0",
    "postcss-syntax": "0.36.2",
    "prettier": "2.8.8",
    "process": "0.11.10",
    "rimraf": "5.0.0",
    "stylelint": "15.6.1",
    "stylelint-config-standard": "33.0.0",
    "stylelint-order": "6.0.3",
    "stylelint-prettier": "3.0.0",
    "typescript": "5.0.4"
  },
  "resolutions": {
    "@parcel/runtime-js@2.8.3": "patch:@parcel/runtime-js@npm%3A2.8.3#./.yarn/patches/@parcel-runtime-js-npm-2.8.3-a82379279f.patch",
    "cordova-windows/shelljs": "0.8.5"
  },
  "packageManager": "yarn@3.5.0"
}
