{
  "name": "munchkin-web",
  "version": "5.1.4",
  "main": "dist/js/app.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf coverage dist doc messages site",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config=webpack/dev.babel.js",
    "dist": "cross-env NODE_ENV=production webpack --config=webpack/dist.babel.js",
    "doc": "rimraf doc && jsdoc src -r -d doc",
    "i18n:download": "node scripts/i18n/download.js",
    "i18n:translate": "node scripts/i18n/translate.js",
    "i18n:upload": "node scripts/i18n/upload.js",
    "lint": "eslint . --ext .js,.jsx",
    "precommit": "npm run lint && npm test",
    "prepush": "npm run lint && npm test",
    "site": "cross-env NODE_ENV=production webpack --config=webpack/site.babel.js",
    "start": "cross-env NODE_ENV=production webpack-dev-server --config=webpack/site.babel.js",
    "stats": "cross-env NODE_ENV=production webpack --config=webpack/stats.babel.js",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch"
  },
  "engines": {
    "node": ">=8"
  },
  "jest": {
    "notify": true,
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(history/es|lodash-es|munchkin-core|react-router/es|react-router-dom/es|react-router-redux/es|redux/es|redux-form/es)/)"
    ]
  },
  "devDependencies": {
    "axios": "0.17.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.1.0",
    "babel-loader": "7.1.2",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-react-intl": "2.4.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-version-inline": "1.0.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-register": "6.26.0",
    "classnames": "2.2.5",
    "cname-webpack-plugin": "1.0.1",
    "connected-react-router": "4.3.0",
    "copy-webpack-plugin": "4.3.1",
    "create-react-class": "15.6.2",
    "cross-env": "5.1.3",
    "eslint": "4.16.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "file-loader": "1.1.6",
    "history": "4.7.2",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.1.4",
    "jest-localstorage-mock": "2.2.0",
    "jsdoc": "3.5.5",
    "jss-camel-case": "6.0.0",
    "jss-default-unit": "8.0.2",
    "jss-global": "3.0.0",
    "jss-nested": "6.0.1",
    "jss-props-sort": "6.0.0",
    "jss-vendor-prefixer": "8.0.0",
    "lodash-es": "4.17.4",
    "material-ui": "1.0.0-beta.30",
    "material-ui-icons": "1.0.0-beta.17",
    "munchkin-core": "5.0.7",
    "offline-plugin": "4.9.0",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hammerjs": "1.0.1",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.0.0-beta.18",
    "react-intl": "2.4.0",
    "react-loadable": "5.3.1",
    "react-redux": "5.0.6",
    "react-responsive": "4.0.3",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "react-sortable-hoc": "0.6.8",
    "react-swipeable-views": "0.12.12",
    "react-swipeable-views-utils": "0.12.11",
    "react-test-renderer": "16.2.0",
    "react-transition-group": "2.2.1",
    "recompose": "0.26.0",
    "redux": "3.7.2",
    "redux-devtools-extension": "2.13.2",
    "redux-form": "7.2.1",
    "redux-form-material-ui": "5.0.0-beta.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "rimraf": "2.6.2",
    "string-replace-loader": "1.3.0",
    "taffydb": "2.7.3",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "2.9.2",
    "webpack-dev-server": "2.7.1",
    "webpack-merge": "4.1.1",
    "webpack-notifier": "1.5.1"
  }
}
