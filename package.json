{
  "private": true,
  "version": "14.6.12",
  "type": "module",
  "packageManager": "yarn@3.6.0",
  "workspaces": [
    "cordova",
    "web"
  ],
  "scripts": {
    "clean": "rimraf .eslintcache .parcel-cache .stylelintcache coverage messages node_modules/.cache parcel-bundle-reports screenshots tsconfig.tsbuildinfo",
    "eslint": "NODE_OPTIONS=\"--max-old-space-size=4096\" eslint --cache \"**/*.{js,json,ts,tsx}\"",
    "i18n:compile": "formatjs compile-folder --ast --format=simple languages languages/generated",
    "i18n:extract": "formatjs extract --out-file=messages.json \"web/src/**/*.{js,ts,tsx}\" && formatjs compile --out-file=languages/en.json messages.json && rm messages.json && prettier --write languages/en.json",
    "lint": "yarn run tsc && yarn run eslint && yarn run stylelint",
    "postinstall": "husky install",
    "prettier": "prettier --cache --ignore-unknown --write .",
    "reinstall": "rimraf node_modules yarn.lock && yarn install",
    "screenshots": "node scripts/screenshots.js",
    "stylelint": "stylelint --cache \"**/*.css\"",
    "test": "jest",
    "tsc": "tsc",
    "g:rimraf": "cd $INIT_CWD && rimraf"
  },
  "devDependencies": {
    "@formatjs/cli": "6.1.3",
    "@jest/globals": "29.6.4",
    "@parcel/transformer-webmanifest": "2.9.3",
    "@swc/core": "1.3.81",
    "@swc/jest": "0.2.29",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "axios": "1.5.0",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-perfectionist": "1.5.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unicorn": "48.0.1",
    "husky": "8.0.3",
    "jest": "29.6.4",
    "jest-environment-jsdom": "29.6.4",
    "lint-staged": "14.0.1",
    "playwright": "1.37.1",
    "postcss-html": "1.5.0",
    "postcss-syntax": "0.36.2",
    "prettier": "3.0.3",
    "process": "0.11.10",
    "rimraf": "5.0.1",
    "stylelint": "15.10.3",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "6.0.3",
    "stylelint-prettier": "4.0.2",
    "typescript": "5.2.2"
  },
  "resolutions": {
    "@parcel/runtime-js@2.9.3": "patch:@parcel/runtime-js@npm%3A2.9.3#./.yarn/patches/@parcel-runtime-js-npm-2.9.3-95e94e0d1e.patch",
    "cordova-windows/shelljs": "0.8.5"
  }
}
