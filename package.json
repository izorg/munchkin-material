{
  "name": "munchkin",
  "private": true,
  "version": "14.0.0",
  "type": "module",
  "targets": {
    "default": {
      "distDir": "web",
      "source": "src/index.html"
    }
  },
  "alias": {
    "@formatjs/icu-messageformat-parser": "@formatjs/icu-messageformat-parser/no-parser"
  },
  "workspaces": [
    "cordova"
  ],
  "scripts": {
    "analyze": "npm run build:parcel -- --reporter @parcel/reporter-bundle-analyzer",
    "build": "npm run build:parcel && npm run build:workbox",
    "build:parcel": "rm -rf web/* && parcel build",
    "build:workbox": "workbox generateSW workbox.config.cjs",
    "clean": "rm -rf .parcel-cache coverage messages node_modules/.cache parcel-bundle-reports screenshots web",
    "dev": "parcel --port=3000 --target=default src/index.html",
    "eslint": "eslint --cache \"**/*.{js,json,ts,tsx}\"",
    "i18n:compile": "formatjs compile-folder --ast --format=simple languages languages/generated",
    "i18n:extract": "formatjs extract --out-file=messages.json \"src/**/*.{js,ts,tsx}\" && formatjs compile --out-file=languages/en.json messages.json && rm messages.json && prettier --write languages/en.json",
    "lint": "npm run tsc && npm run eslint && npm run stylelint",
    "prepare": "husky install",
    "prettier": "prettier --cache --ignore-unknown --write .",
    "reinstall": "rm -rf cordova/node_modules node_modules package-lock.json && npm install",
    "screenshots": "node scripts/screenshots.js",
    "start": "firebase emulators:start",
    "stylelint": "stylelint --cache \"**/*.css\"",
    "test": "jest",
    "tsc": "tsc"
  },
  "dependencies": {
    "@dnd-kit/core": "6.0.8",
    "@dnd-kit/modifiers": "6.0.1",
    "@dnd-kit/sortable": "7.0.2",
    "@dnd-kit/utilities": "3.2.1",
    "@emotion/cache": "11.10.5",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@formatjs/intl-datetimeformat": "6.4.3",
    "@formatjs/intl-getcanonicallocales": "2.0.5",
    "@formatjs/intl-locale": "3.0.11",
    "@formatjs/intl-numberformat": "8.3.3",
    "@formatjs/intl-pluralrules": "5.1.8",
    "@mdi/js": "7.1.96",
    "@mui/material": "5.11.9",
    "@mui/utils": "5.11.9",
    "@reduxjs/toolkit": "1.9.2",
    "@sentry/react": "7.38.0",
    "clsx": "1.2.1",
    "core-js": "3.28.0",
    "firebase": "9.17.1",
    "framer-motion": "7.10.3",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.2.8",
    "react-redux": "8.0.5",
    "react-router-dom": "6.8.1",
    "react-transition-group": "4.4.5",
    "redux": "4.2.1",
    "redux-undo": "1.0.1",
    "stylis": "4.1.3",
    "stylis-plugin-rtl": "2.1.1",
    "uuid": "9.0.0",
    "workbox-window": "6.5.4"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.10.6",
    "@emotion/eslint-plugin": "11.10.0",
    "@formatjs/cli": "6.0.1",
    "@jest/globals": "29.4.3",
    "@parcel/packager-raw-url": "2.8.3",
    "@parcel/reporter-bundle-analyzer": "2.8.3",
    "@parcel/transformer-webmanifest": "2.8.3",
    "@types/cordova": "11.0.0",
    "@types/dom-screen-wake-lock": "1.0.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/stylis": "4.0.2",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "axios": "1.3.3",
    "babel-plugin-formatjs": "10.3.36",
    "babel-plugin-polyfill-corejs3": "0.6.0",
    "babel-plugin-react-remove-properties": "0.3.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "lint-staged": "13.1.2",
    "parcel": "2.8.3",
    "playwright": "1.30.0",
    "postcss-html": "1.5.0",
    "postcss-syntax": "0.36.2",
    "prettier": "2.8.4",
    "process": "0.11.10",
    "stylelint": "15.2.0",
    "stylelint-config-standard": "30.0.1",
    "stylelint-order": "6.0.2",
    "stylelint-prettier": "2.0.0",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5",
    "workbox-cli": "6.5.4"
  },
  "overrides": {
    "cordova-windows": {
      "shelljs": "0.8.5"
    }
  }
}
