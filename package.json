{
  "name": "munchkin-web",
  "version": "2.0.7",
  "main": "./dist/js/app.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config=webpack/config.development.js",
    "dist": "cross-env NODE_ENV=production webpack --config=webpack/config.production.js",
    "doc": "esdoc",
    "i18n:download": "node scripts/i18n/download.js",
    "i18n:translate": "node scripts/i18n/translate.js",
    "i18n:upload": "node scripts/i18n/upload.js",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:js": "eslint --ext .js,.jsx .",
    "site": "cross-env NODE_ENV=production webpack --config=webpack/config.site.js",
    "start": "cross-env NODE_ENV=production webpack-dev-server --config=webpack/config.site.js",
    "stats": "cross-env NODE_ENV=production webpack --config=webpack/config.stats.js",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "engines": {
    "node": ">=8"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "./src/constants/",
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "notify": true,
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "transformIgnorePatterns": [
      "node_modules/(?!(munchkin-core)/)"
    ]
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "axios": "0.16.2",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-version-inline": "1.0.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.24.1",
    "classnames": "2.2.5",
    "cname-webpack-plugin": "1.0.1",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "enzyme": "2.9.1",
    "esdoc": "0.5.2",
    "eslint": "4.3.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-css-modules": "2.7.3",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "history": "4.6.3",
    "html-webpack-plugin": "2.29.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.4",
    "jest-enzyme": "3.6.1",
    "lodash": "4.17.4",
    "material-ui": "0.18.7",
    "munchkin-core": "2.0.7",
    "normalize.css": "7.0.0",
    "offline-plugin": "4.8.3",
    "postcss-cssnext": "3.0.2",
    "postcss-loader": "2.0.6",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-helmet": "5.1.3",
    "react-hot-loader": "3.0.0-beta.7",
    "react-intl": "2.3.0",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.2",
    "react-router-redux": "5.0.0-alpha.6",
    "react-sortable-hoc": "0.6.6",
    "react-swipeable-views": "0.12.5",
    "react-swipeable-views-utils": "0.12.5",
    "react-tap-event-plugin": "2.0.1",
    "react-tappable": "1.0.0",
    "react-test-renderer": "15.6.1",
    "react-transition-group": "2.2.0",
    "redux-form": "7.0.3",
    "redux-thunk": "2.2.0",
    "style-loader": "0.18.2",
    "stylelint": "8.0.0",
    "stylelint-config-css-modules": "1.1.0",
    "stylelint-config-standard": "17.0.0",
    "stylelint-webpack-plugin": "0.9.0",
    "webpack": "3.4.1",
    "webpack-bundle-analyzer": "2.8.3",
    "webpack-dev-server": "2.6.1",
    "webpack-merge": "4.1.0",
    "webpack-notifier": "1.5.0"
  }
}
