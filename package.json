{
  "name": "munchkin-web",
  "version": "13.3.7",
  "scripts": {
    "analyze": "cross-env ANALYZE=true npm run build:web",
    "build:cordova": "cross-env NODE_ENV=production BUILD=cordova webpack",
    "build:web": "cross-env NODE_ENV=production BUILD=web webpack",
    "clean": "rm -rf coverage dist messages node_modules/.cache screenshots web",
    "dev": "cross-env NODE_ENV=development BUILD=web webpack serve",
    "eslint": "eslint --cache \"**/*.{js,json,mjs,ts,tsx}\"",
    "i18n:compile": "formatjs compile-folder --ast --format=simple languages languages/generated",
    "i18n:extract": "formatjs extract --out-file=messages.json \"src/**/*.{js,ts,tsx}\" && formatjs compile --out-file=languages/en.json messages.json && rm messages.json && prettier --write languages/en.json",
    "lint": "npm run tsc && npm run eslint && npm run stylelint",
    "prepare": "husky install",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "screenshots": "cross-env NODE_ENV=test node scripts/screenshots.mjs",
    "start": "cross-env NODE_ENV=production BUILD=web webpack-dev-server",
    "stylelint": "stylelint --cache \"**/*.{css,html}\"",
    "test": "jest",
    "tsc": "tsc"
  },
  "dependencies": {
