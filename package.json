{
  "name": "munchkin-web",
  "version": "13.0.1",
  "scripts": {
    "build:cordova": "cross-env NODE_ENV=production BUILD=cordova webpack",
    "build:web": "cross-env NODE_ENV=production BUILD=web webpack",
    "clean": "rm -rf coverage dist messages node_modules/.cache screenshots web",
    "dev": "cross-env NODE_ENV=development BUILD=web webpack serve",
    "eslint": "eslint . --cache --ext=.js,.json,.jsx,.mjs,.ts,.tsx",
    "i18n:compile": "node -r=dotenv/config scripts/i18n/download.mjs && prettier --write=languages",
    "i18n:extract": "formatjs extract --id-interpolation-pattern=[contenthash:5] --out-file=messages.json \"src/**/*.{js,jsx,ts,tsx}\" && formatjs compile --out-file=languages/en.json messages.json && rm messages.json && prettier --write=languages/en.json",
    "lint": "npm run eslint && npm run stylelint",
    "prepare": "husky install",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "screenshots": "cross-env NODE_ENV=test node scripts/screenshots.mjs",
    "start": "cross-env NODE_ENV=production BUILD=web webpack-dev-server",
    "stats": "cross-env STATS=true npm run build:web",
    "stylelint": "stylelint --cache=\"**/*.{css,jsx}\"",
    "test": "jest",
    "tsc": "tsc"
  },
  "dependencies": {
    "@emotion/cache": "11.4.0",
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@material-ui/core": "5.0.0-alpha.37",
    "@reduxjs/toolkit": "1.6.0",
    "@sentry/react": "6.7.2",
    "clsx": "1.1.1",
    "core-js": "3.15.1",
    "deepmerge": "4.2.2",
    "firebase": "8.6.8",
    "formdata-polyfill": "3.0.20",
    "framer-motion": "4.1.17",
    "history": "5.0.0",
    "mdi-material-ui": "6.22.1",
    "prop-types": "15.7.2",
    "qs": "6.10.1",
    "react": "17.0.2",
    "react-beautiful-dnd": "13.1.0",
    "react-dom": "17.0.2",
    "react-intl": "5.14.1",
    "react-redux": "7.2.4",
    "react-router-dom": "6.0.0-beta.0",
    "react-sortablejs": "6.0.0",
    "react-transition-group": "4.4.2",
    "redux": "4.1.0",
