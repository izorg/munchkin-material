{
  "private": true,
  "name": "@munchkin/web",
  "type": "module",
  "scripts": {
    "analyze": "yarn run build:parcel --reporter @parcel/reporter-bundle-analyzer",
    "build": "yarn run build:parcel && yarn run build:workbox",
    "build:parcel": "yarn g:rimraf dist && parcel build",
    "build:workbox": "workbox generateSW workbox.config.json",
    "dev": "parcel --port=3000",
    "start": "firebase emulators:start"
  },
