    "@types/stylis": "4.2.1",
    "@types/uuid": "9.0.5",
    "firebase-tools": "12.7.0",
    "parcel": "2.10.0",
    "process": "0.11.10",
    "typescript": "5.2.2",
    "workbox-cli": "7.0.0"
  },
  "alias": {
    "@formatjs/icu-messageformat-parser": "@formatjs/icu-messageformat-parser/no-parser"
  },
  "targets": {
    "default": {
      "source": "src/index.html"
