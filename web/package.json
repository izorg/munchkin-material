{
  "private": true,
  "name": "@munchkin/web",
  "type": "module",
  "scripts": {
    "analyze": "yarn run build:parcel --reporter @parcel/reporter-bundle-analyzer",
    "build": "yarn run build:parcel && yarn run build:workbox",
    "build:parcel": "yarn g:rimraf dist && parcel build",
    "build:workbox": "workbox generateSW workbox.config.json",
    "dev": "cross-env DISABLE_V8_COMPILE_CACHE=1 parcel --port=3000",
    "start": "firebase emulators:start"
  },
  "dependencies": {
    "@dnd-kit/core": "6.0.8",
    "@dnd-kit/modifiers": "6.0.1",
    "@dnd-kit/sortable": "7.0.2",
    "@dnd-kit/utilities": "3.2.1",
    "@emotion/cache": "11.10.7",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@formatjs/intl-datetimeformat": "6.6.0",
    "@formatjs/intl-getcanonicallocales": "2.1.0",
    "@formatjs/intl-locale": "3.2.0",
    "@formatjs/intl-numberformat": "8.4.0",
    "@formatjs/intl-pluralrules": "5.2.0",
    "@mdi/js": "7.2.96",
    "@mui/material": "5.12.1",
    "@mui/utils": "5.12.0",
    "@reduxjs/toolkit": "1.9.5",
    "@sentry/react": "7.48.0",
    "clsx": "1.2.1",
    "core-js": "3.30.1",
    "firebase": "9.19.1",
    "framer-motion": "10.12.2",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.4.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.10.0",
    "react-transition-group": "4.4.5",
    "redux": "4.2.1",
    "redux-undo": "1.0.1",
    "scrollyfills": "1.0.0",
    "stylis": "4.1.3",
    "stylis-plugin-rtl": "2.1.1",
    "swr": "2.1.3",
    "uuid": "9.0.0",
    "workbox-window": "6.5.4"
  },
  "devDependencies": {
    "@mui/types": "7.2.4",
    "@parcel/packager-raw-url": "2.8.3",
    "@parcel/reporter-bundle-analyzer": "2.8.3",
    "@parcel/transformer-webmanifest": "2.8.3",
    "@types/dom-screen-wake-lock": "1.0.1",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@types/stylis": "4.0.2",
    "@types/uuid": "9.0.1",
    "cross-env": "7.0.3",
    "parcel": "2.8.3",
    "process": "0.11.10",
    "typescript": "5.0.4",
    "workbox-cli": "6.5.4"
  },
  "alias": {
    "@formatjs/icu-messageformat-parser": "@formatjs/icu-messageformat-parser/no-parser"
  },
  "targets": {
    "default": {
      "source": "src/index.html"
    }
  }
}
