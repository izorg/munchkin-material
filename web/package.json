    "firebase": "10.5.0",
    "framer-motion": "10.16.4",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.5.0",
    "react-redux": "8.1.3",
    "react-router-dom": "6.17.0",
    "react-transition-group": "4.4.5",
    "redux": "4.2.1",
